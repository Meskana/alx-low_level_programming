 * _strcat - concatenates two strings dest and src
 * @dest: to the  destination
 * @src: from the source
 * Return: return character
 */
char *_strcat(char *dest, char *src)
{
	int l1 = 0, l2 = 0, i;

	l1 = strlen(dest);
	l2 = strlen(src);
	for (i = 0; i <= l2; i++)
	{
		dest[l1 + i]  = src[i];
	}
	return (dest);
